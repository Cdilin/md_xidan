

.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 20rpx;
    height: 220rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .order_value{
    position: relative;
    display:block;
    width: auto;
    height: 50rpx;
    line-height: 50rpx;
    overflow: auto;
    font-size: 0.9rem;
}

.cart-list .order_value_inline{
    position: relative;
    display:inline-block;
    width: auto;
    height: 50rpx;
    line-height: 50rpx;
    overflow: auto;
    font-size: 0.9rem;
}
.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}